{
  "dependencies": {
    "@axe-core/playwright": "^4.8.5",
    "@cucumber/pretty-formatter": "^1.0.0",
    "axe-html-reporter": "^2.2.3",
    "cross-env": "^7.0.3",
    "dotenv": "^16.4.1",
    "fs-extra": "^11.1.1",
    "playwright": "^1.39.0",
    "ts-node": "^10.9.1"
  },
  "devDependencies": {
    "@cucumber/cucumber": "^10.3.1",
    "@playwright/test": "^1.41.2",
    "allure-commandline": "^2.27.0",
    "allure-cucumberjs": "^2.12.0",
    "allure-js-commons": "^2.12.0",
    "allure-playwright": "^2.12.0"
  },
  "scripts": {
    "pretest": "npx ts-node src/utilities/init.ts",
    "filecount": "npx ts-node src/utilities/fileCount.ts",
    "test": "npm run build && cross-env test_env=dev cucumber-js test",
    "tests:tagged": "node ./node_modules/.bin/cucumber-js  --tags @Test",
    "test_cmd_line": "cucumber-js test",
    "allure": "allure generate --single-file -c -o  allure-reports",
    "axe_report": "cat ./accessibility-reports/artifacts/*.html >> ./accessibility-reports/artifacts/FinalAccessibilityReport.html"
  }
}
