{
  "name": "uec_capacity_management",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@hookform/resolvers": "^3.9.1",
    "nhsuk-frontend": "^8.0.2",
    "nhsuk-react-components": "^4.1.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.53.1",
    "react-router-dom": "^6.27.0",
    "react-scripts": "5.0.1",
    "web-vitals": "^2.1.4",
    "yup": "^1.4.0"
  },
  "scripts": {
    "start": "DISABLE_ESLINT_PLUGIN=true react-scripts start",
    "prebuild": "node increment-version.js",
    "build": "DISABLE_ESLINT_PLUGIN=true react-scripts build",
    "test": "react-scripts test",
    "test:ci": "npm run test -- --coverage --watchAll=false",
    "test:coverage": "npm run test -- --coverage --watchAll=true",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
    "devDependencies": {
      "@axe-core/react": "^4.10.0",
      "@testing-library/jest-dom": "^6.6.2",
      "@testing-library/react": "^16.0.1",
      "@testing-library/user-event": "^14.5.2",
      "@types/jest": "^27.5.2",
      "@types/node": "^16.18.68",
      "@types/react": "^18.3.12",
      "@types/react-dom": "^18.3.1",
      "@types/react-router-dom": "^5.3.3",
      "@typescript-eslint/eslint-plugin": "^8.12.1",
      "@typescript-eslint/parser": "^8.12.1",
      "eslint": "^9.12.0",
      "eslint-plugin-jest": "^28.8.3",
      "eslint-plugin-jsx-a11y": "^6.10.0",
      "eslint-plugin-react": "^7.37.1",
      "globals": "^15.11.0",
      "ts-jest": "^29.2.5",
      "typescript": "^5.6.3"
    },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.test.{js,jsx,ts,tsx}",
      "!src/index.tsx",
      "!src/pages/test.tsx",
      "!src/reportWebVitals.ts",
      "!<rootDir>/node_modules/",
      "!<rootDir>/src/prototype/**"
    ],
    "coveragePathIgnorePatterns": [
      "<rootDir>/src/prototype/"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 80,
        "functions": 80,
        "lines": 80,
        "statements": 80
      }
    },
    "coverageReporters": [
      "text",
      "lcov"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    },
    "moduleNameMapper": {
      "\\.(css|less|sass|scss)$": "identity-obj-proxy",
      "\\.(gif|ttf|eot|svg)$": "identity-obj-proxy"
    }
  }
}
